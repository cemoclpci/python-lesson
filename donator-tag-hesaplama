tag_fiyati = in(input("Tag fiyati giriniz: "))
isminiz = input("Kullanıcı adınızı giriniz: ")
oynama_saati = in(input("Oynama saatinizi giriniz"))

if (tag_fiyati >= 3000):
    if(oynama_saati >= 150)
    print("Tag almak için saatiniz ve bakiyeniz yetiyor.")
    else:
    print(f'{isminiz} tag almak için oynama saatiniz yetmiyor.')
else:
print(f'{isminiz} tag almak için yeterli bakiyeniz bulunmuyor')
